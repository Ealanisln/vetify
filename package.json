{
  "name": "vetify",
  "version": "1.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "implementation:check": "node scripts/update-implementation-status.mjs",
    "dev": "next dev",
    "prebuild": "npx prisma generate",
    "build": "prisma generate && next build",
    "vercel-build": "prisma generate && next build",
    "vercel-build-clean": "prisma generate && next build --config next.config.vercel.js",
    "vercel-build-original": "rm -rf .next && node scripts/create-vercel-tsconfig.mjs && prisma generate && SKIP_TYPE_CHECK=true next build --no-lint",
    "vercel-build-fallback": "rm -rf .next && node scripts/create-vercel-tsconfig-fallback.mjs && prisma generate && SKIP_TYPE_CHECK=true next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "css:verify": "node scripts/css-verification.mjs",
    "css:check": "pnpm build && pnpm css:verify",
    "assets:copy": "node scripts/copy-assets.mjs",
    "test": "node scripts/env-config.mjs localhost && jest",
    "test:watch": "node scripts/env-config.mjs localhost && jest --watch",
    "test:unit": "node scripts/env-config.mjs localhost && jest __tests__/unit",
    "test:integration": "node scripts/env-config.mjs localhost && jest --config jest.integration.config.ts",
    "test:security": "jest __tests__/unit/security",
    "test:performance": "jest __tests__/unit/performance",
    "test:components": "jest __tests__/unit/components",
    "test:coverage": "node scripts/env-config.mjs localhost && jest --coverage",
    "test:coverage:watch": "node scripts/env-config.mjs localhost && jest --coverage --watch",
    "test:coverage:report": "node scripts/test-coverage-report.mjs",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "pnpm test:unit && pnpm test:integration && pnpm test:e2e",
    "test:critical": "node scripts/run-critical-tests.mjs",
    "test:pre-deploy": "pnpm test:critical && pnpm build",
    "env:show": "node scripts/env-config.mjs show",
    "env:localhost": "node scripts/env-config.mjs localhost",
    "env:localip": "node scripts/env-config.mjs localip",
    "env:safari": "node scripts/env-config.mjs safari",
    "env:development": "node scripts/env-config.mjs development",
    "env:help": "node scripts/env-config.mjs help",
    "vercel:env": "node scripts/vercel-env-export.mjs development",
    "vercel:env:dev": "node scripts/vercel-env-export.mjs development",
    "vercel:env:prod": "node scripts/vercel-env-export.mjs production",
    "vercel:env:help": "node scripts/vercel-env-export.mjs help",
    "mvp:checklist": "node scripts/mvp-launch-checklist.mjs",
    "mvp:setup": "node scripts/mvp-production-setup.mjs",
    "build:production": "NODE_ENV=production prisma generate && prisma migrate deploy && next build",
    "start:production": "NODE_ENV=production next start",
    "db:migrate:production": "prisma migrate deploy",
    "health:check": "node scripts/health-check.mjs",
    "db:seed:production": "node scripts/seed-production.mjs",
    "db:verify": "npx tsx scripts/verify-vercel-connection.ts",
    "stripe:verify": "node scripts/stripe-setup-verification.mjs",
    "stripe:setup": "node scripts/stripe-products-setup.mjs",
    "stripe:cleanup": "npx tsx scripts/cleanup-duplicate-subscriptions.ts",
    "webhook:debug": "node scripts/webhook-debug.mjs",
    "pricing:sync": "node scripts/verify-pricing-sync.mjs",
    "trial:test": "node scripts/test-trial-flow.mjs",
    "trial:status": "node scripts/check-subscription-status.mjs",
    "trial:status:states": "node scripts/check-subscription-status.mjs --states",
    "trial:fix": "npx tsx scripts/fix-trial-status.ts",
    "migrate-docs": "node scripts/migrate-docs-simple.mjs",
    "gitbook:serve": "gitbook serve",
    "gitbook:build": "gitbook build",
    "gitbook:install": "npm install -g gitbook-cli",
    "docs:setup": "npm run gitbook:install && gitbook init",
    "docs:dev": "cd gitbook-docs && gitbook serve",
    "docs:build": "cd gitbook-docs && gitbook build",
    "docs:deploy": "gitbook build && echo 'Build complete. Deploy to your hosting platform.'",
    "docs:install": "npm install -g gitbook-cli",
    "docs:clean": "cd gitbook-docs && rm -rf _book && gitbook build",
    "docs:pdf": "cd gitbook-docs && gitbook pdf . ../vetify-docs.pdf",
    "docs:epub": "cd gitbook-docs && gitbook epub . ../vetify-docs.epub",
    "docs:serve": "cd gitbook-docs && gitbook serve --port 4000",
    "organize:docs": "node organize-docs-v2.js",
    "clean:docs": "node clean-docs.js",
    "sentry:quiet": "node scripts/sentry-logging-control.mjs quiet",
    "sentry:normal": "node scripts/sentry-logging-control.mjs normal",
    "sentry:verbose": "node scripts/sentry-logging-control.mjs verbose",
    "sentry:debug": "node scripts/sentry-logging-control.mjs debug",
    "sentry:status": "node scripts/sentry-logging-control.mjs status",
    "deploy:security:fix": "node scripts/fix-production-security.mjs",
    "deploy:production": "node scripts/production-deploy.mjs",
    "deploy:check": "node scripts/production-deploy.mjs --check-only",
    "supabase:connections": "node scripts/generate-supabase-connections.mjs",
    "vercel:env:generate": "node scripts/generate-vercel-env.mjs",
    "vercel:env:custom": "node scripts/generate-vercel-env-custom.mjs",
    "vercel:env:optimized": "node scripts/generate-vercel-env-optimized.mjs",
    "prepare": "husky"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@fullcalendar/core": "^6.1.17",
    "@fullcalendar/daygrid": "^6.1.17",
    "@fullcalendar/interaction": "^6.1.17",
    "@fullcalendar/list": "^6.1.17",
    "@fullcalendar/react": "^6.1.17",
    "@fullcalendar/timegrid": "^6.1.17",
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.0.1",
    "@kinde-oss/kinde-auth-nextjs": "2.8.0",
    "@prisma/client": "6.10.1",
    "@react-email/components": "^1.0.1",
    "@sentry/nextjs": "^10.7.0",
    "@sentry/node": "^10.7.0",
    "@sentry/profiling-node": "^10.7.0",
    "@stripe/stripe-js": "^7.4.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.4",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.3",
    "@vercel/og": "^0.8.5",
    "cloudinary": "^2.8.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "exceljs": "^4.4.0",
    "framer-motion": "^12.6.3",
    "import-in-the-middle": "^1.14.2",
    "jsonwebtoken": "^9.0.3",
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.7",
    "lucide-react": "^0.487.0",
    "nanoid": "^5.1.5",
    "next": "15.5.9",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.4.4",
    "qrcode.react": "^4.2.0",
    "react": "19.1.0",
    "react-day-picker": "^9.7.0",
    "react-dom": "19.1.0",
    "react-email": "^5.0.5",
    "react-hook-form": "^7.56.4",
    "recharts": "^3.0.2",
    "require-in-the-middle": "^7.5.2",
    "resend": "^4.7.0",
    "sonner": "^2.0.5",
    "square": "43.2.1",
    "stripe": "^16.12.0",
    "tailwind-merge": "^3.3.1",
    "yet-another-react-lightbox": "^3.25.0",
    "zod": "^3.25.36"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@faker-js/faker": "^10.1.0",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/minimatch": "^6.0.0",
    "@types/node": "^20",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@types/superagent": "^8.1.9",
    "@types/supertest": "^6.0.3",
    "autoprefixer": "^10.4.21",
    "chalk": "^5.4.1",
    "dotenv": "^17.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.4.8",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.1.1",
    "jest-mock-extended": "^4.0.0",
    "lint-staged": "^16.2.7",
    "msw": "^2.10.5",
    "playwright": "^1.57.0",
    "postcss": "^8",
    "prisma": "^6.5.0",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.18",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "pnpm": {
    "overrides": {
      "jose": "^4.15.5",
      "@types/react": "19.1.8",
      "@types/react-dom": "19.1.6",
      "qs": ">=6.14.1"
    }
  },
  "packageManager": "pnpm@10.20.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ],
    "*.{js,mjs}": [
      "eslint --fix"
    ]
  }
}
